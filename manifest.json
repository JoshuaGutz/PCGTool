{
      "manifest_version": 3,
      "name": "PCGTool",
      "version": "1.0",
      "content_scripts": [
        {
          "matches": [
            "https://www.twitch.tv/popout/*/extensions/pm0qkv9g4h87t5y6lg329oam8j7ze9/panel", // pattern for main URL of extension (deemonrider)
            "https://*.ext-twitch.tv/*" // pattern for both source iframe URLs of extension
          ],
          "js": ["content.js"],
          "run_at": "document_start", // Other options document_idle document_end
          "all_frames": true
        }
      ]
    }