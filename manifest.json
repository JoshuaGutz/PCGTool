{
      "manifest_version": 3,
      "name": "Extension for PCG Extension",
      "version": "1.0",
      "content_scripts": [
        {
          "matches": [
            "https://www.twitch.tv/popout/*/extensions/*/panel", // Original main page URL pattern
            "https://*.ext-twitch.tv/*", // pattern for both iframes source URLs
          ],
          "js": ["content-final.js"],
          "run_at": "document_start", // Other options document_idle document_end
          "all_frames": true
        }
      ]
    }